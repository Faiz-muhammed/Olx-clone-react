r />\n          <input\n            className=\"input\"\n            type=\"email\"\n            value={email}\n            onChange={(e)=>setEmail(e.target.value)}\n            id=\"fname\"\n            name=\"email\"\n            defaultValue=\"John\"\n          />\n          <br />\n          <label htmlFor=\"lname\">Phone</label>\n          <br />\n          <input\n            className=\"input\"\n            type=\"number\"\n            value={phone}\n            onChange={(e)=>setPhone(e.target.value)}\n            id=\"lname\"\n            name=\"phone\"\n            defaultValue=\"Doe\"\n          />\n          <br />\n          <label htmlFor=\"lname\">Password</label>\n          <br />\n          <input\n            className=\"input\"\n            type=\"password\"\n            value={password}\n            onChange={(e)=>setPassword(e.target.value)}\n            id=\"lname\"\n            name=\"password\"\n            defaultValue=\"Doe\"\n          />\n          <br />\n          <br />\n          <button>Signup</button>\n        </form>\n        <a>Login</a>\n      </div>\n    </div>\n  );\n}\n","/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/firebase/config.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Pages/Login.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Components/Login/Login.js",["105","106"],"import React, { useState, useContext } from 'react';\nimport {FirebaseContext} from '../../store/Context'\nimport Logo from '../../olx-logo.png';\nimport './Login.css';\nimport {useHistory} from 'react-router-dom'\n\nfunction Login() {\n\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n  const {firebase} = useContext(FirebaseContext)\n  const history = useHistory()\n  const handleLogin = (e)=>{\n    e.preventDefault()\n    firebase.auth().signInWithEmailAndPassword(email,password).then(()=>{\n      history.push('/')\n    }).catch((error)=>{\n     alert(error.message)\n    })\n\n  }\n  return (\n    <div>\n      <div className=\"loginParentDiv\">\n        <img width=\"200px\" height=\"200px\" src={Logo}></img>\n        <form onSubmit={handleLogin}>\n          <label htmlFor=\"fname\">Email</label>\n          <br />\n          <input\n            className=\"input\"\n            type=\"email\"\n            id=\"fname\"\n            value={email}\n            onChange={(e)=>setEmail(e.target.value)}\n            name=\"email\"\n            defaultValue=\"John\"\n          />\n          <br />\n          <label htmlFor=\"lname\">Password</label>\n          <br />\n          <input\n            className=\"input\"\n            type=\"password\"\n            value={password}\n            onChange={(e)=>setPassword(e.target.value)}\n            id=\"lname\"\n            name=\"password\"\n            defaultValue=\"Doe\"\n          />\n          <br />\n          <br />\n          <button>Login</button>\n        </form>\n        <a>Signup</a>\n      </div>\n    </div>\n  );\n}\n\nexport default Login;\n","/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/store/Context.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Pages/Create.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Components/Create/Create.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Pages/ViewPost.js",[],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/Components/View/View.js",["107"],"/home/muhammedfaiz/Desktop/Olxclone/React_tutorial_olx_clone/src/store/PostContext.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":37,"column":9,"nodeType":"114","endLine":37,"endColumn":54},{"ruleId":"115","severity":1,"message":"116","line":90,"column":9,"nodeType":"114","endLine":90,"endColumn":12},{"ruleId":"112","severity":1,"message":"113","line":25,"column":9,"nodeType":"